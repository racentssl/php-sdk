{
    "name": "racent/sdk",
    "description": "锐诚SSL的API SDK",
    "license": "MIT",
    "authors": [
        {
            "name": "Xiaohui Lam",
            "email": "op@staff.digital-sign.cn"
        }
    ],
    "autoload": {
        "psr-4": {
            "Racent\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Racent\\Test\\": "tests"
        }
    },
    "require": {
        "php": ">=5.6",
        "guzzlehttp/guzzle": ">4.0",
        "illuminate/support": ">=5.0",
        "psr/http-message": ">=1.0",
        "symfony/http-kernel": "2.*|3.*|4.*|5.*"
    },
    "suggest": {
        "laravel/framework": "在 Laravel 5.5+ 框架上使用, 效果更佳"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Racent\\RacentServiceProvider"
            ]
        }
    }
}
